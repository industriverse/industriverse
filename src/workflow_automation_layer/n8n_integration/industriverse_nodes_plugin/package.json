{
  "name": "n8n-nodes-industriverse",
  "version": "0.1.0",
  "description": "n8n nodes for integrating with Industriverse Workflow Automation Layer",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "industriverse",
    "workflow",
    "automation",
    "industrial"
  ],
  "license": "MIT",
  "homepage": "https://industriverse.io",
  "author": {
    "name": "Industriverse",
    "email": "support@industriverse.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/industriverse/n8n-nodes-industriverse.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write '**/*.ts'",
    "lint": "eslint nodes credentials --ext .ts",
    "lintfix": "eslint nodes credentials --ext .ts --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/IndustriverseTriggerApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/IndustriverseTrigger/IndustriverseTrigger.node.js",
      "dist/nodes/IndustriverseTriggerResponse/IndustriverseTriggerResponse.node.js",
      "dist/nodes/IndustriverseTriggerWorkflow/IndustriverseTriggerWorkflow.node.js",
      "dist/nodes/IndustriverseTriggerGetWorkflowStatus/IndustriverseTriggerGetWorkflowStatus.node.js",
      "dist/nodes/IndustriverseTriggerUpdateTaskStatus/IndustriverseTriggerUpdateTaskStatus.node.js",
      "dist/nodes/IndustriverseTriggerCreateIntervention/IndustriverseTriggerCreateIntervention.node.js",
      "dist/nodes/IndustriverseTriggerGetInterventionStatus/IndustriverseTriggerGetInterventionStatus.node.js",
      "dist/nodes/IndustriverseTriggerUpdateCapsule/IndustriverseTriggerUpdateCapsule.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.18",
    "@types/request-promise-native": "~1.0.18",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-core": "^0.125.0",
    "n8n-workflow": "^0.107.0",
    "prettier": "^2.7.1",
    "typescript": "~4.8.4"
  },
  "dependencies": {
    "axios": "^1.1.3"
  }
}
